{
  "name": "@toastup/core",
  "version": "0.0.4",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./src/": {
      "import": "./src/",
      "require": "./src/"
    },
    "./style": {
      "import": "./src/style.scss",
      "require": "./src/style.scss"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "src"
  ],
  "keywords": [
    "react",
    "toast",
    "message",
    "notification",
    "alert",
    "notification",
    "toastup"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "typings": "dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.3.0",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.6",
    "@vitest/coverage-v8": "^1.2.1",
    "copyfiles": "^2.4.1",
    "csstype": "^3.1.3",
    "dts-bundle-generator": "^9.2.4",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.33",
    "postcss-scss": "^4.0.9",
    "rimraf": "^5.0.5",
    "sass": "^1.70.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.1"
  },
  "license": "MIT",
  "author": "Kamil Bysiec <kamil.bysiec@agileplayers.com>",
  "homepage": "https://toastup.kbysiec.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kbysiec/toastup.git"
  },
  "bugs": {
    "url": "https://github.com/kbysiec/toastup/issues"
  }
}
